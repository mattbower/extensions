<h1>A/B Testing Results</h1>

<pop:ab:results>

  <h2><pop:title/></h2>

  <table>
    <thead>
      <tr>
        <th>Version</th><th>Displays</th><th>Wins</th>
      </tr>
    </thead>
    <tbody>
      <pop:variations name="example-test" break="tr">
        <td><pop:name/></td>
        <td><pop:picks/></td>
        <td><pop:wins/></td>
      </pop:variations>
    </tbody>
  </table>

  <p><pop:result/></p>

</pop:ab:results>
